<%= __FILE__ %>
<h2>Courier information</h2>
<table>
<tbody>
  <tr>
    <td> courier name  </td>
    <td> courier email </td>  
  </tr>
  <tr>
    <td><%= courier.name %></td>
    <td><%= courier.email %></td>  
  </tr>
</tbody>
</table>




<div>
  <button>
    <%= link_to '<<back', couriers_index_path  %>
  </button>
  <button>
    <%= link_to 'edit>>', edit_path %>
  </button>
</div>


<table>
  <thead>
    <tr>
      <th>Tracking number</th>
      <th>Delivery status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deliveryInfo.each do |package| %>
      <tr>
        <td><%= package.tracking_number %></td>
        <td><%= package.delivery_status %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<button>
  <%= link_to 'add delivery', delivery_path %>
</button>

